{
    "name": "scientistcloud/portal",
    "description": "ScientistCloud Data Portal",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "auth0/auth0-php": "^8.17",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^2.0",
        "guzzlehttp/promises": "^2.0",
        "php-http/discovery": "^1.19",
        "psr/http-factory": "^1.0",
        "psr/http-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ScientistCloud\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}